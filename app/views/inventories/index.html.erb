<h1>Inventories</h1>

<div id="inventories">
  <% @inventories.each do |inventory| %>

    <p>
      <%= link_to inventory_path(inventory), class: "text-decoration-none text-dark d-block" do%>
        <%= render inventory %>
      <% end %>
    </p>
  <% end %>
</div>

<%= link_to "New inventory", new_inventory_path %>
